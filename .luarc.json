{
  "runtime.version": "LuaJIT",
  "runtime.path": ["lua/?.lua", "lua/?/init.lua"],
  "workspace.checkThirdParty": false,
  "workspace.library": [
    "/usr/share/nvim/runtime/lua",
    "C:\\Program Files\\Neovim\\share\\nvim\\runtime\\lua"
  ],
  "diagnostics.disable": [
    "undefined-field",
    "param-type-mismatch",
    "unused-local",
    "deprecated",
    "duplicate-set-field",
    "redefined-local"
  ],
  "diagnostics.globals": ["nvim_bufferline", "_lazygit_toggle", "vim"]
}
