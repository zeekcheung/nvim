{
  "runtime.version": "LuaJIT",
  "runtime.path": [
    "lua/?.lua",
    "lua/?/init.lua"
  ],
  "workspace.checkThirdParty": false,
  "workspace.library": [
    "$VIMRUNTIME",
    "./lua"
  ],
    "diagnostics.disable": [
      "undefined-field",
      "param-type-mismatch",
      "unused-local",
      "deprecated",
      "duplicate-set-field"
    ],
    "diagnostics.globals": [
        "nvim_bufferline",
        "_lazygit_toggle"
    ]
}